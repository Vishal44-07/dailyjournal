<%- include('partials/header.ejs'); -%>
    <main>
        <h1>Home</h1>
    </main>
    <section>
        <div>
            <p>
                <%= paragraph1 %>
            </p>
            <% newContent.forEach(function(element){ %>
                <h2>
                    <%= element.title %>
                </h2>
                <p>
                    <%= element.content.substring(0, 100) + "...." %>
                    <a href="/posts/<%= element.title %>">Read More</a>
                </p> 
                <% }) %>
        </div>
    </section>

    <%- include('partials/footer.ejs'); -%>